<div class="row">
  <div class="col-md-6 offset-md-3">

      <h1> <%= @user.username %>
      </h1>

    <ul class="nav nav-pills nav-justified">

        <li class="nav-item">
      <%= link_to 'Posts', user_path(@user.username), class: "nav-link" %>
        </li>

        <li class="nav-item">
      <%= link_to 'Liked', liked_photos_path(@user.username), class: "nav-link" %>
        </li>

        <li class="nav-item">
      <%= link_to 'Feed', user_feed_path(@user.username), class: "nav-link" %>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="#">Followers</a>
        </li>

        <li class="nav-item">
      <%= link_to 'Following', user_following_path(@user.username), class: "nav-link" %>
        </li>
    </ul>

  </div>
</div>


 <%= current_user.own_photos.count %>

  <% current_user.own_photos.each do |z| %>
    <%= render "photos/photo", giraffe: z %>
    
<% end %>
